    "@backstage/test-utils": "^0.1.13",
    "@backstage/theme": "^0.2.8",
    "@backstage/integration-react": "^0.1.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "history": "^5.0.0",
    "react": "^16.13.1",
