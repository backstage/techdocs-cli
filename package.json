{
  "name": "@techdocs/cli",
  "description": "CLI for running TechDocs locally.",
  "version": "0.1.3",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/backstage/techdocs-cli",
    "directory": "."
  },
  "keywords": [
    "backstage",
    "techdocs"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.cjs.js",
  "scripts": {
    "build": "./bin/build.sh",
    "lint": "backstage-cli lint",
    "test": "backstage-cli test --passWithNoTests",
    "clean": "backstage-cli clean",
    "start": "nodemon --",
    "prettier:check": "prettier --check ."
  },
  "bin": {
    "techdocs-cli": "bin/techdocs-cli"
  },
  "devDependencies": {
    "@spotify/prettier-config": "^9.0.0",
    "@types/serve-handler": "^6.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": "@spotify/prettier-config",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "files": [
    "bin",
    "dist"
  ],
  "nodemonConfig": {
    "watch": "./src",
    "exec": "bin/build && bin/techdocs-cli",
    "ext": "ts"
  },
  "dependencies": {
    "@backstage/cli": "^0.2.0",
    "commander": "^6.1.0",
    "fs-extra": "^9.0.1",
    "http-proxy": "^1.18.1",
    "react-dev-utils": "^10.2.1",
    "serve-handler": "^6.1.3"
  }
}
